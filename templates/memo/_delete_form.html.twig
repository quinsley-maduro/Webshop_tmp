<form method="post" action="{{ path('m_e_m_o_delete', {'id': m_e_m_o.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ m_e_m_o.id) }}">
    <button class="btn">Delete</button>
</form>
